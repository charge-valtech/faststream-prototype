<script>
  $(function(){
    var schemePrefArray = ['Empty'],
        preferencesAsText = [
                              'First preference',
                              'Second preference',
                              'Third preference',
                              'Fourth preference'
                            ];

    $('[data-schemecheck]').on('change', function() {
      var $this = $(this),
          thisScheme = $this.closest('.grid-wrapper').find('.heading-small').text(),
          thisSchemeID = $this.attr('id'),
          arrayPosition = $.inArray(thisSchemeID, schemePrefArray),
          emptyPosition = $.inArray('Empty', schemePrefArray);

      if(arrayPosition >= 0) {
        //Do nothing
      } else if($this.is(':checked')) {
        if(emptyPosition < 0) {
          schemePrefArray.push(thisSchemeID);
        } else {
          schemePrefArray.splice(emptyPosition, 1, thisSchemeID);
        }
        var arrayPositionNow = $.inArray(thisSchemeID, schemePrefArray);
        $this.next('[data-selecttext]').text(preferencesAsText[arrayPositionNow]);

        $('#selectedPrefList li').eq(arrayPositionNow).after('<li data-scheme-id="' + thisSchemeID + '"><h3 class="bold-small">' + preferencesAsText[arrayPositionNow] + '</h3>' + thisScheme);
      }

      if(!$this.is(':checked')) {
        schemePrefArray.splice(arrayPosition, 1, 'Empty');
        $this.next('[data-selecttext]').text('Select scheme');
        $('#selectedPrefList').find('[data-scheme-id="' + thisSchemeID + '"]').remove();
      }

      console.log(schemePrefArray);


    });

  });
</script>